<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Express stock photo application.</p>
    <div id="photos">
    	<% photos.forEach(function(photo){ %>
    		<div class="photo">
    			<h2 class="photo"><%= photo.name %></h2>
    			<img src="<%= photo.path %>" alt="<%= photo.name %>" title="<%= photo.name %>" />
    		</div>
    		<% } )%>
    </div>
  </body>
</html>
